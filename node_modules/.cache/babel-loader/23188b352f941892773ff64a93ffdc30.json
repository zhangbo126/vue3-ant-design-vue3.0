BA,qBA8BA;AAAA;;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,sBAAA,EADA,CAGA;;AACA,IAAA,MAAA,CAAA,OAAA,CAAA,UAAA,CAAA;AAAA,aAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,CAAA,CAAA,CAAA;AAAA,KAAA,EAJA,CAMA;;AACA,SAAA,MAAA,CAAA,OAAA,EAAA,YAAA;AACA,MAAA,KAAA,CAAA,KAAA,IAAA,KAAA,CAAA,IAAA,CAAA,cAAA,CAAA,IAAA,CAAA,KAAA,CAAA,KAAA,EAAA,MAAA,CAAA,CAAA;AACA,KAFA;AAGA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <a-modal\r\n    title=\"新建规则\"\r\n    :width=\"640\"\r\n    :visible=\"visible\"\r\n    :confirmLoading=\"loading\"\r\n    @ok=\"() => { $emit('ok') }\"\r\n    @cancel=\"() => { $emit('cancel') }\"\r\n  >\r\n    <a-spin :spinning=\"loading\">\r\n      <a-form :form=\"form\" v-bind=\"formLayout\">\r\n        <!-- 检查是否有 id 并且大于0，大于0是修改。其他是新增，新增不显示主键ID -->\r\n        <a-form-item v-show=\"model && model.id > 0\" label=\"主键ID\">\r\n          <a-input v-decorator=\"['id', { initialValue: 0 }]\" disabled />\r\n        </a-form-item>\r\n        <a-form-item label=\"描述\">\r\n          <a-input v-decorator=\"['description', {rules: [{required: true, min: 5, message: '请输入至少五个字符的规则描述！'}]}]\" />\r\n        </a-form-item>\r\n      </a-form>\r\n    </a-spin>\r\n  </a-modal>\r\n</template>\r\n\r\n<script>\r\nimport pick from 'lodash.pick'\r\n\r\n// 表单字段\r\nconst fields = ['description', 'id']\r\n\r\nexport default {\r\n  props: {\r\n    visible: {\r\n      type: Boolean,\r\n      required: true\r\n    },\r\n    loading: {\r\n      type: Boolean,\r\n      default: () => false\r\n    },\r\n    model: {\r\n      type: Object,\r\n      default: () => null\r\n    }\r\n  },\r\n  data () {\r\n    this.formLayout = {\r\n      labelCol: {\r\n        xs: { span: 24 },\r\n        sm: { span: 7 }\r\n      },\r\n      wrapperCol: {\r\n        xs: { span: 24 },\r\n        sm: { span: 13 }\r\n      }\r\n    }\r\n    return {\r\n      form: this.$form.createForm(this)\r\n    }\r\n  },\r\n  created () {\r\n    console.log('custom modal created')\r\n\r\n    // 防止表单未注册\r\n    fields.forEach(v => this.form.getFieldDecorator(v))\r\n\r\n    // 当 model 发生改变时，为表单设置值\r\n    this.$watch('model', () => {\r\n      this.model && this.form.setFieldsValue(pick(this.model, fields))\r\n    })\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/list/modules"}]}